{
  "name": "PROJECT",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "karma start build/karma.conf.js",
    "test:watch": "karma start build/karma.conf.js --no-single-run",
    "test:chrome:watch": "karma start build/karma.conf.js --no-single-run --browsers Chrome",
    "prebuild": "npm run clean && npm run test",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "start": "node build/webpack.dev.server.js"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "core-js": "2.4.1",
    "cross-env": "2.0.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "1.0.1",
    "karma-mocha": "1.1.1",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "phantomjs-prebuilt": "2.1.8",
    "rimraf": "2.5.4",
    "ts-loader": "0.8.2",
    "tslint": "3.13.0",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "dependencies": {
    "eye-oh-see": "git+https://github.com/ZachBray/eye-oh-see.git",
    "react": "15.2.1",
    "react-dom": "15.2.1"
  }
}
