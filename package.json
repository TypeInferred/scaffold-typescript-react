{
  "name": "PROJECT",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "test": "karma start build/test/karma.conf.js",
    "test:watch": "yarn run test -- --no-single-run",
    "test:chrome:watch": "yarn run test:watch -- --browsers Chrome",
    "gen:css:ts": "cd src && tsm",
    "gen:css:ts:watch": "yarn run gen:css:ts:watch -- --watch",
    "prebuild": "yarn run clean && yarn run test && yarn run gen:css:ts",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "prestart": "yarn run gen:css:ts",
    "start": "node build/webpack.dev.server.js",
    "precommit": "yarn run test"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.2.4",
    "chai": "3.5.0",
    "core-js": "2.4.1",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "husky": "0.11.9",
    "ignore-loader": "0.1.2",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "1.8.0",
    "mocha": "3.2.0",
    "phantomjs-prebuilt": "2.1.13",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "ts-loader": "1.2.2",
    "tslint": "4.0.2",
    "tslint-loader": "3.2.1",
    "typed-stylus-modules": "0.1.16",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "dependencies": {
    "eye-oh-see": "0.4.5",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  }
}
